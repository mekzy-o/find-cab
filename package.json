{
  "name": "find-cab",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "watch": "tsc -w",
    "start:dev": "DEBUG=development && nodemon build/index.js",
    "start": "node build/index.js",
    "copy:templates": "cpx './src/emailTemplates/**' 'build/emailTemplates'",
    "deploy": "git push heroku main",
    "test": "mocha -r ts-node/register src/tests/*.spec.ts --timeout 20000 --exit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "model:generate": "./node_modules/.bin/sequelize model:generate",
    "migration:generate": "./node_modules/.bin/sequelize migration:generate",
    "seed:generate": "./node_modules/.bin/sequelize seed:generate",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "undo:migration": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "undo:seed": "./node_modules/.bin/sequelize db:seed:undo:all",
    "db:rollback": "yarn undo:migration && yarn undo:seed",
    "migrate:seed": "yarn db:rollback && yarn migrate && yarn seed",
    "docker:build": "docker build -t express-sequelize-typescript-boilerplate .",
    "docker:run": "docker run --rm -it -p 8080:8080 express-sequelize-typescript-boilerplate"
  },
  "dependencies": {
    "argon2": "^0.28.2",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "heroku-logger": "^0.3.3",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.1.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.2",
    "nodemailer-express-handlebars": "^4.0.0",
    "pg": "^8.3.2",
    "sequelize": "4",
    "sequelize-cli": "^6.2.0",
    "winston": "^3.3.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/heroku-logger": "^1.0.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mocha": "^8.0.3",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-express-handlebars": "^4.0.1",
    "@types/sequelize": "4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
